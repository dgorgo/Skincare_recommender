   "source": [
    "def find_matches(ingredient, chemicals):\n",
    "    for chemical in chemicals:\n",
    "        if chemical.lower() in ingredient.lower():\n",
    "            return chemical\n",
    "    return None"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "69a0dbe9",
   "metadata": {},
   "source": [
    "Join the 2 datasets as one\n"
   ]
  },
