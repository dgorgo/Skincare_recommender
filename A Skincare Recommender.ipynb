   "cell_type": "code",
   "execution_count": 9,
   "id": "074475af",
   "metadata": {},
   "outputs": [],
   "source": [
    "products['matched_chemical'] = products['clean_ingreds'].apply(lambda x: find_matches(x, chemicals_df['Chemical Name']))"
   ]
  },
