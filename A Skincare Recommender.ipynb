   "source": [
    "skin_type_ingredients = chemicals_df['Chemical Name']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "74fe11af",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Iterate through the chemicals and update skin_type_ingredients dictionary\n",
    "for index, row in chemicals_df.iterrows():\n",
    "    skin_types = row['Skin Type'].split(', ')\n",
    "    for skin_type in skin_types:\n",
    "        skin_type = skin_type.strip()  # Removes any leading/trailing whitespace\n",
    "        if skin_type in skin_type_ingredients:\n",
    "            skin_type_ingredients[skin_type].append(row['Chemical Name'])"
   ]
  },
